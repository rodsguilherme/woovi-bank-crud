{
    "version": 2,
    "builds": [
        {
            "src": "dist/graphql.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        },
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/graphql.js"
        }
    ]
}